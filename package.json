{
  "name": "react-lab",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "deploy": "npm run build && wrangler deploy",
    "dev": "react-router dev",
    "start": "wrangler dev",
    "typecheck": "react-router typegen && tsc -b",
    "lint": "eslint",
    "format": "prettier --write .",
    "typegen": "pnpm run /^typegen:.*/",
    "typegen:react-router": "react-router typegen",
    "typegen:cloudflare": "wrangler types"
  },
  "dependencies": {
    "@react-router/fs-routes": "7.3.0",
    "@react-router/serve": "7.3.0",
    "isbot": "5.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "7.3.0"
  },
  "devDependencies": {
    "@react-router/dev": "7.3.0",
    "@tailwindcss/vite": "4.0.14",
    "@types/node": "22",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@virtual-live-lab/eslint-config": "2.2.18",
    "@virtual-live-lab/prettier-config": "2.0.17",
    "@virtual-live-lab/tsconfig": "2.1.18",
    "eslint": "9.22.0",
    "prettier": "3.5.3",
    "tailwindcss": "4.0.14",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1",
    "vite": "6.2.2",
    "vite-tsconfig-paths": "5.1.4",
    "wrangler": "4.0.0"
  },
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": "22"
  }
}
